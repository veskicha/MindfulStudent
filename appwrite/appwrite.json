{
    "projectId": "tcs-design-project-mindfulstudent",
    "projectName": "MindfulStudent",
    "databases": [
        {
            "$id": "database",
            "name": "Database"
        }
    ],
    "collections": [
        {
            "$id": "users",
            "name": "Users",
            "databaseId": "database",
            "enabled": true,
            "documentSecurity": false,
            "indexes": [],
            "attributes": [
                {
                    "key": "username",
                    "type": "string",
                    "size": 32,
                    "required": true,
                    "array": false
                }
            ]
        }
    ],
    "functions": [
        {
            "$id": "auto-user-management",
            "name": "User management (Auto)",
            "runtime": "node-16.0",
            "execute": [],
            "events": [
                "users.*.create",
                "users.*.delete"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "ignore": [
                "node_modules",
                ".npm"
            ],
            "path": "functions/auto-user-management"
        }
    ]
}